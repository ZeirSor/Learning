     "text": [
      "hello world!\n",
      "hello world!\n",
      "hello, world!\n",
      "hello, world!\n",
      "hello , world\n",
      "hello,  world!\n"
     ]
    }
   ],
   "source": [
    "print(\"hello world!\")\n",
    "print('hello world!')\n",
    "print('hello, ' +  'world!')\n",
    "print(\"hello, \" + \"world!\")\n",
    "# 格式化字符串\n",
    "print('{}, {}'.format('hello ', 'world')) # 占位符\n",
    "print('%s, %s!' % ('hello', ' world'))    # %s"
   ]
  },
